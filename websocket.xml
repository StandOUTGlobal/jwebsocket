<?xml version="1.0" encoding="utf-8"?>
<websocket xmlns="http://dev.w3.org/html5/websockets/">
	<configurations>
		<host>websocket-server</host>
		<port>8080</port>
		<documentRoot>/</documentRoot>
	</configurations>
	<handlers>
		<handler>
			<handlerName>org.jwebsocket.example.TestHandler</handlerName>
			<properties>
				<property>
					<key>appName</key>
					<value>TestHandler</value>
				</property>
			</properties>
		</handler>
	</handlers>
</websocket>